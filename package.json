        "laravel-mix": "^5.0",
        "postcss": "^8.4.5",
        "tailwindcss": "^3.0.16"
    },
    "dependencies": {
        "highlight.js": "^10.4.1",
        "markdown-it": "^12.0.3",
        "moment": "^2.29.1",
        "postcss-import": "^12.0.0",
        "postcss-nested": "^4.0.0",
        "shiki": "^0.10.0"
    }
